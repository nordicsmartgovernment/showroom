<?xml version="1.0" encoding="UTF-8"?>
<Document xmlns="urn:iso:std:iso:20022:tech:xsd:camt.053.001.08">
  <BkToCstmrStmt>
    <GrpHdr>
      <MsgId>$MessageID$</MsgId>
      <CreDtTm>$DateTimeNow$</CreDtTm>
      <MsgRcpt>
        <Nm>$BuyerName$</Nm>
        <Id>
          <OrgId>
            <Othr>
              <Id>$BuyerID$</Id>
              <SchmeNm>
                <Cd>BANK</Cd>
              </SchmeNm>
            </Othr>
          </OrgId>
        </Id>
      </MsgRcpt>
    </GrpHdr>
    <Stmt>
      <Id>$MessageID$</Id>
      <LglSeqNb>$SeqNumber$</LglSeqNb>
      <CreDtTm>$DateTimeNow$</CreDtTm>
      <FrToDt>
        <FrDtTm>$DateTimePurchaseFrom$</FrDtTm>
        <ToDtTm>$DateTimePurchaseTo$</ToDtTm>
      </FrToDt>
      <Acct>
        <Id>
          <IBAN>$BuyerIBAN$</IBAN>
        </Id>
        <Ccy>$Currency$</Ccy>
        <Nm>Business account</Nm>
        <Ownr>
          <Nm>$BuyerName$</Nm>
          <Id>
            <OrgId>
              <Othr>
                <Id>$BuyerID$</Id>
              </Othr>
            </OrgId>
          </Id>
        </Ownr>
        <Svcr>
          <FinInstnId>
            <BICFI>NDEADKKK</BICFI>
            <Nm>NORDEA BANK DANMARK A/S</Nm>
          </FinInstnId>
        </Svcr>
      </Acct>
      <Bal>
        <Tp>
          <CdOrPrtry>
            <Cd>OPBD</Cd>
          </CdOrPrtry>
        </Tp>
        <Amt Ccy="$Currency$">0.00</Amt>
        <CdtDbtInd>DBIT</CdtDbtInd>
        <Dt>
          <Dt>$DateNow$</Dt>
        </Dt>
      </Bal>
      <TxsSummry>
        <TtlCdtNtries>
          <Sum>0.00</Sum>
        </TtlCdtNtries>
        <TtlDbtNtries>
          <Sum>$TotalPrice$</Sum>
        </TtlDbtNtries>
      </TxsSummry>
      <Ntry>
        <Amt Ccy="$Currency$">$TotalPrice$</Amt>
        <CdtDbtInd>DBIT</CdtDbtInd>
        <Sts>
          <Prtry>BOOK</Prtry>
        </Sts>
        <BookgDt>
          <Dt>$DateNow$</Dt>
        </BookgDt>
        <ValDt>
          <Dt>$DateNow$</Dt>
        </ValDt>
        <BkTxCd>
          <Domn>
            <Cd>PMNT</Cd>
            <Fmly>
              <Cd>RCDT</Cd>
              <SubFmlyCd>ESCT</SubFmlyCd>
            </Fmly>
          </Domn>
          <Prtry>
            <Cd>700</Cd>
            <Issr>FFFS</Issr>
          </Prtry>
        </BkTxCd>
        <NtryDtls>
          <TxDtls>
            <AmtDtls>
              <TxAmt>
                <Amt Ccy="$Currency$">$TotalPrice$</Amt>
              </TxAmt>
            </AmtDtls>
            <RltdPties>
              <Cdtr>
                <Pty>
                  <Nm>$SellerName$</Nm>
                </Pty>
              </Cdtr>
            </RltdPties>
            <RmtInf>
              <Strd>
                <RfrdDocInf>
                  <Nb>$InvoiceID$</Nb>
                </RfrdDocInf>
                <CdtrRefInf>
                  <Tp>
                    <CdOrPrtry>
                      <Cd>SCOR</Cd>
                    </CdOrPrtry>
                  </Tp>
                  <Ref>$PaymentReference$</Ref>
                </CdtrRefInf>
              </Strd>
            </RmtInf>
          </TxDtls>
        </NtryDtls>
      </Ntry>
    </Stmt>
  </BkToCstmrStmt>
</Document>
